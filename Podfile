source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '9.0'
inhibit_all_warnings!

target :ChristiePortal do
    pod 'SWRevealViewController', '~>2.3'
    pod 'AFNetworking', '~> 2.5'
    pod 'AFNetworkActivityLogger', '~> 2.0'
    pod 'DateTools', '~> 1.7'
    pod 'TPKeyboardAvoiding','~> 1.2'
    pod 'SSKeychain'
    pod 'Reachability'
    pod 'FMDB', '~> 2.5'
    pod 'MJNIndexView'
    pod 'BlocksKit', '~> 2.2'
    pod 'CSLinearLayoutView', '~> 1.0'
    pod 'MKMapView-ZoomLevel'
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'GoogleMaps', '~> 1.10.5'
end

target :ChristiePortalTests do
    pod 'OCMock', '~> 3.2'
end

link_with 'ChristiePortal', 'ChristiePortalTests'
